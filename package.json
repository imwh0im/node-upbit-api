{
  "name": "node-upbit-api",
  "version": "1.0.0",
  "description": "Wrapper for upbit API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:type": "tsc -p tsconfig.type.json",
    "build": "rm -rf ./dist && tsc -p tsconfig.json && npm run build:type",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imwh0im/node-upbit-api.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imwh0im/node-upbit-api/issues"
  },
  "homepage": "https://github.com/imwh0im/node-upbit-api#readme",
  "keywords": [
    "bitcoin",
    "bitcoin api",
    "cryptocurrency",
    "upbit",
    "upbit api",
    "upbit sdk",
    "upbit typescript",
    "upbit node"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "jsonwebtoken": "^8.5.1",
    "query-string": "^7.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
